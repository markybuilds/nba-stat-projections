# API and Backend URLs
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_WEBSOCKET_URL=ws://localhost:8001

# CDN Configuration
NEXT_PUBLIC_CDN_ENABLED=false
NEXT_PUBLIC_CDN_URL=https://your-cdn-url.com

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Social Login Providers
# To enable social login, configure your providers in the Supabase dashboard
# 1. Go to Authentication > Providers
# 2. Enable and configure each provider with your OAuth credentials
# 3. Set the callback URL to: https://your-app-url.com/auth/callback
#
# GitHub: https://github.com/settings/developers
# Google: https://console.cloud.google.com/apis/credentials

# Other settings
ANALYZE=false 